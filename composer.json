{
  "name": "joaoroyosilva/phppimaco",
  "description": "Lib fork (PronerInformatica/phppimaco) para geração de Etiquetas PIMACO",
  "keywords": [
    "PIMACO",
    "etiquetas",
    "tags",
    "hang tags"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Diogo Bemfica",
      "email": "diogo.fragabemfica@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Proner\\PhpPimaco\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "mpdf/mpdf": "^8.0.10",
    "endroid/qr-code": "^4.5",
    "picqer/php-barcode-generator": "^0.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Proner\\PhpPimacoTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.2"
  }
}
